{
  "manifest_version": 3,
  "name": "CRXJS React Vite Example",
  "version": "1.0.0",
  "action": { "default_popup": "index.html" },
  "permissions": ["scripting", "activeTab"],
  "web_accessible_resources": [
    {
      "resources": ["src/scripts/siteinfo.js"],
      "matches": ["<all_urls>"]
    }
  ]
}